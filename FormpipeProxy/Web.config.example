<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net"/>
  </configSections>

  <appSettings>
    <add key="owin:AutomaticAppStartup" value="false" />
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="FormPipeUsername" value="användarnamn"/>
    <add key="FormPipePassword" value="lösenord"/>
  </appSettings>

  <log4net>
    <appender name="RollingFile" type="log4net.Appender.RollingFileAppender">
      <file value="C:\inetpub\logs\FormPipeProxy\PROD\fp.log" />
      <appendToFile value="true" />
      <maximumFileSize value="100MB" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date %level [%REQID] - %logger - %message%newline" />
        <converter>
          <name value="REQID" />
          <type value="FormpipeProxy.Util.RequestIdConverter" />
        </converter>
      </layout>
    </appender>

    <appender name="RollingFileLOB" type="log4net.Appender.RollingFileAppender">
      <file value="C:\inetpub\logs\FormPipeProxy\PROD\fp-lob.log" />
      <appendToFile value="true" />
      <maximumFileSize value="100MB" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date %level [%REQID] - %logger - %message%newline" />
        <converter>
          <name value="REQID" />
          <type value="FormpipeProxy.Util.RequestIdConverter" />
        </converter>
      </layout>
    </appender>

    <logger name="LOB" additivity="false">
      <level value="All" />
      <appender-ref ref="RollingFileLOB" />
    </logger>

    <root>
      <level value="All" />
      <appender-ref ref="RollingFile" />
    </root>
  </log4net>

	<system.web>
    <compilation debug="true" targetFramework="4.7.2" />
    <httpRuntime targetFramework="4.7.2" />
  </system.web>
  
  <system.webServer>
    <modules runAllManagedModulesForAllRequests="true" />
